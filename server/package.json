{
  "name": "scrum-master-server",
  "version": "1.0.0",
  "description": "Backend API for Daily Scrum Master Selector",
  "type": "module",
  "main": "server.mjs",
  "scripts": {
    "start": "node server.mjs",
    "dev": "node --watch server.mjs",
    "migrate": "node migrations/migrate.mjs",
    "migrate:rollback": "node migrations/migrate.mjs rollback",
    "seed": "node seeds/content-seed.mjs",
    "setup": "npm run migrate && npm run seed",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "scrum",
    "standup",
    "rotation",
    "team-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}